<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd"
             version="6">

    <application-name>Dependency-Analysis</application-name>
    <display-name>Dependency-Analysis</display-name>

    <module>
        <ejb>causeway-core.jar</ejb>
    </module>
    <module>
        <ejb>causeway-rest.jar</ejb>
    </module>
    <module>
        <web>
            <web-uri>causeway-web.war</web-uri>
            <context-root>causeway</context-root>
        </web>
    </module>

    <library-directory>lib</library-directory>
</application>

